	<servlet>
		<servlet-name>DummyServlet</servlet-name>
		<servlet-class>com.mobile.ivr.test.auto.controller.DummyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DummyServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
